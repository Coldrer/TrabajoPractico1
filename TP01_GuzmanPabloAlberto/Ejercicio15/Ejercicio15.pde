int numero1, numero2, suma, resta, multiplicacion;
float division;

void setup(){
  numero1 = 15;
  numero2 = 3;
  suma = numero1 + numero2;
  resta = numero1 - numero2;
  multiplicacion = numero1 * numero2;
  division = numero1 / numero2;
  println("La suma es " + suma + ", la resta es " + resta + ", la multiplicacion es " + multiplicacion + " y la division es " + division);
}
